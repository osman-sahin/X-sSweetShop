@model IEnumerable<Category>

<li class="dropdown nav-item">
    <a asp-controller="Pie" asp-action="Index" class="dropdown-toggle nav-link" data-toggle="dropdown">Pies</a>
    <ul class="dropdown-menu">
        @foreach (var category in Model)
        {
            <li>
                <a class="dropdown-item" asp-controller="Pie" asp-action="List" asp-route-category="@category.CategoryName">@category.CategoryName</a>
            </li>
        }
        <li class="dropdown-divider"></li>
        <li>
            <a class="dropdown-item" asp-controller="Pie" asp-action="List" asp-route-category="">View all pies</a>
        </li>
    </ul>
</li>